<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextFamily">
        <item name="fontFamily">@font/main_medium</item>
        <item name="android:fontFamily">@font/main_medium</item>
    </style>

    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/bg_spinner</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="fab_rounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="fab_style">
        <item name="android:background">@drawable/bg_50</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="CustomProgressBar" >
        <item name="android:indeterminateDrawable">@drawable/loading_progressbar</item>
        <item name="android:animationResolution">33</item>

    </style>
</resources>